<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default" namespace="/">
        <action name="" class="huyvq.struts2.StartAction">
            <result name="success">searchForUser.jsp</result>
            <result name="success2">searchForStaff.jsp</result>
            <result name="fail">login.html</result>
        </action>
        <action name="start" class="huyvq.struts2.StartAction">
            <result name="success">searchForUser.jsp</result>
            <result name="success2">searchForStaff.jsp</result>
            <result name="fail">login.html</result>
        </action>
        
    </package>
</struts>
